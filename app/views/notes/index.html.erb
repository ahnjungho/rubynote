<div class="container">
  <div>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
  </div>
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-6 col-md-offset-3">

<ul>
  <% @notes.each do |note| %>
  <li>
    <div><%= note.body %></div>
    <p><%= note.user.username %> at <%= note.created_at %></p>
  </li>
  <% end %>
</ul>

    </div>
  </div>
</div>


